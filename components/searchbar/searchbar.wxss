    width:100%;
    height:100rpx;
    padding-left:20rpx;
    padding-right:20rpx;
    background: #fff;
    border-top: 1rpx solid #F7F7F7;
    position: fixed;
    left:0;
    top:0;
    z-index: 200;
  }
  .content {
    height: 88rpx;
    padding: 0 18rpx;
    background: rgba(255,255,255,1);
    border-radius: 30rpx;
    border: 2rpx solid rgba(220,222,226,1);
    flex: 1;
    display: flex;
    align-items: center;
  }
  .search-icon {
    width: 34rpx;
    height: 34rpx;
    margin-bottom: -4rpx;
  }
  .input {
    flex: 1;
    margin: 0 20rpx;
  }
  .delete-icon {
    width: 40rpx;
    height: 40rpx;
  }
  .text-btn {
    margin-left: 24rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(21,96,220,1);
  }
  